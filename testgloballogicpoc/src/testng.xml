<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Main Test Suite" parallel="tests" verbose="1">
    <test name="Grid firefox Test">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="MobileAutomation.testcases.ParellelExecutions"/>
            <class name="MobileAutomation.testcases.ParallelExecution2"/>
        </classes>
    </test>
    <test name="Grid chrome Test">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="MobileAutomation.testcases.ParellelExecutions"/>
            <class name="MobileAutomation.testcases.ParallelExecution2"/>
        </classes>
    </test>
    <test name="Grid Android Test">
        <parameter name="browser" value="Android"/>
        <classes>
            <class name="MobileAutomation.testcases.ParellelExecutions"/>
            <class name="MobileAutomation.testcases.ParallelExecution2"/>
        </classes>
    </test>
</suite>